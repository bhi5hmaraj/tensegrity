        CO3[Tensegrity:<br/>Real-time coupling metrics,<br/>automated constraints,<br/>contract enforcement]
    end

    subgraph "4. Learning Governance"
        L1[Problem:<br/>Human understanding<br/>lags behind code]
        L2[Traditional:<br/>Documentation<br/>goes stale immediately]
        L3[Tensegrity:<br/>Active learning primitives,<br/>prediction challenges,<br/>knowledge gap tracking]
    end

    subgraph "5. Equilibrium Governance"
        E1[Problem:<br/>Balance velocity,<br/>quality, coherence,<br/>learning for context]
        E2[Traditional:<br/>Fixed policies<br/>startup = enterprise]
        E3[Tensegrity:<br/>Tunable force profiles,<br/>adaptive mechanisms,<br/>context-aware]
    end

